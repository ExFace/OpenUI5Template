{
	"name" : "exface/OpenUI5Template",
	"description" : "UI-Template for the ExFace business web-app plattform based on the SAP OpenUI5 library",
	"type" : "library",
	"license" : "GPL-3.0+",
	"authors" : [{
			"name" : "Andrej Kabachnik",
			"email" : "kabachnik@gmail.com",
			"role" : "Creator"
		}
	],
	"support" : {
		"issues" : "https://github.com/exface/OpenUI5Template/issues"
	},
	"require" : {
		"exface/core" : "~0.1",
		"bower-asset/openui5-sap.ui.core": "^1.5",
		"bower-asset/openui5-sap.ui.unified": "^1.5",
		"bower-asset/openui5-sap.ui.table": "^1.5",
		"bower-asset/openui5-sap.ui.commons": "^1.5",
		"bower-asset/openui5-sap.f": "^1.5",
		"bower-asset/openui5-sap.m": "^1.5",
		"bower-asset/openui5-sap.tnt": "^1.5",
		"bower-asset/openui5-themelib_sap_belize": "^1.5",
		"bower-asset/font-awesome-openui5": "^v2.1"
	},
    "autoload": {
        "psr-4": { "exface\\OpenUI5Template\\": "" },
        "exclude-from-classmap": [
            "/Config/",
            "/Translations/",
            "/Model/"
        ]
    }
}